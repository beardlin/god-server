			<id>mvnrepository</id>
			<name>mvnrepository</name>
			<url>http://192.168.0.231:8081/nexus/content/groups/public/</url>
			<layout>default</layout>
			<releases>
				<enabled>true</enabled>
			</releases>
			<snapshots>
				<enabled>false</enabled>
			</snapshots>
		</repository>
	</repositories>

