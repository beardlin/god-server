    </select>
    
    <!-- 查询所有不为null的权限 -->
    <select id="queryAllPermission" resultType="java.lang.String">
        SELECT permission 
        FROM sys_menu WHERE del_flag = 0  
        and permission IS NOT NULL 
    </select>
    <!-- 修改儿子们的父亲名称 -->
    <update id="updateChildParentName" parameterType="map" >
        update sys_menu set parent_name = #{menu.menuName}
        where parent_id = #{menu.id}
    </update>
    
    <!-- 根据菜单id集合查询所有对应权限标识的集合 -->
    <select id="ByidList" parameterType="java.util.List" resultType="java.lang.String">
        SELECT permission
        FROM sys_menu WHERE del_flag = 0 and permission IS NOT NULL and permission &lt;&gt; '' and id in
           <foreach collection="list" open="(" close=")" item="temp" separator=",">
            #{temp}
           </foreach>
    </select>
	
</mapper>