  <select id="getLogListPage" parameterType="map" resultMap="LogModel">
	SELECT o.* , u.user_name FROM `sys_log` o  LEFT JOIN `sys_user` u ON o.update_by = u.login_name  
	WHERE o.del_flag = 0 
