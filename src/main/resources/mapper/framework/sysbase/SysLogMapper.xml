       AND o.title LIKE concat('%',#{log.title,jdbcType=VARCHAR},'%') 
    </if>
    <if test="log.startDate != null and log.startDate != ''">
<!--        AND SUBSTR(o.create_date FROM 1 FOR 10) &gt;= #{log.startDate,jdbcType=VARCHAR} -->
       AND o.create_date &gt;= #{log.startDate,jdbcType=VARCHAR}
    </if>
    <if test="log.endDate != null and log.endDate != ''">
<!--        AND SUBSTR(o.create_date FROM 1 FOR 10) &lt;= #{log.endDate,jdbcType=VARCHAR} -->
       AND o.create_date &lt;= #{log.endDate,jdbcType=VARCHAR}
    </if>
    <if test="log.type != null and log.type != ''">
       AND o.type = #{log.type,jdbcType=CHAR}
    </if>
